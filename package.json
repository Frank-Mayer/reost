{
  "name": "reost",
  "repository": "https://github.com/Reost-MC/reost-website.git",
  "author": "Frank Mayer <frank.mayer1907@protonmail.com>",
  "license": "MIT",
  "private": false,
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "last 3 and_chr versions",
    "last 3 chrome versions",
    "last 3 opera versions",
    "last 3 ios_saf versions",
    "last 3 safari versions"
  ],
  "scripts": {
    "dev": "parcel serve \"src/**/*.html\"",
    "clean": "rm -rf ./.parcel-cache && rm -rf ./dist",
    "build": "parcel build \"src/index.html\" \"src/**/*.html\" \"src/hotlink-io/*\" --no-source-maps --no-cache --public-url \"https://reost.de/\"",
    "pb": "photon './dist'"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.3.x",
    "@parcel/transformer-postcss": "2.3.x",
    "autoprefixer": "^10.4.4",
    "parcel": "2.3.x",
    "photon-cli": "^0.4.0",
    "postcss": "^8.4.12",
    "sass": "^1.50.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@frank-mayer/magic": "^3.0.0",
    "photon-re": "^0.3.3",
    "bootstrap-icons": "^1.8.1",
    "normalize.css": "^8.0.1"
  }
}
