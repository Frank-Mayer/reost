{
  "name": "reost",
  "repository": "https://github.com/Reost-MC/reost-website.git",
  "author": "Frank Mayer <frank.mayer1907@protonmail.com>",
  "license": "MIT",
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "last 3 and_chr versions",
    "last 3 chrome versions",
    "last 3 opera versions",
    "last 3 ios_saf versions",
    "last 3 safari versions"
  ],
  "scripts": {
    "lint": "eslint --fix --ext .ts .",
    "dev": "parcel serve \"src/**/*.html\"",
    "clean": "rm -rf ./.parcel-cache && rm -rf ./dist/*",
    "pre-b": "node ./html-proc.mjs",
    "build": "parcel build \"src/index.html\" \"src/**/*.html\" \"src/hotlink-io/*\" --no-source-maps --no-cache --public-url \"https://reost.de/\"",
    "test": "parcel build \"src/index.html\" \"src/**/*.html\" \"src/hotlink-io/*\" --no-source-maps --no-cache && node --experimental-specifier-resolution=node ./html-proc.mjs",
    "post-b": "photon \"./dist\""
  },
  "devDependencies": {
    "@frank-mayer/yahp": "^0.2.1",
    "@octokit/rest": "^18.12.0",
    "@parcel/transformer-postcss": "2.3.x",
    "@parcel/transformer-sass": "2.3.x",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "autoprefixer": "^10.4.5",
    "eslint": "^8.14.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.26.0",
    "parcel": "2.3.x",
    "photon-cli": "^0.4.0",
    "postcss": "^8.4.13",
    "querystring-es3": "^0.2.1",
    "sass": "^1.51.0",
    "showdown": "^2.1.0",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "@frank-mayer/magic": "^4.0.0",
    "bootstrap-icons": "^1.8.1",
    "firebase": "^9.8.1",
    "normalize.css": "^8.0.1",
    "photon-re": "^0.3.3"
  }
}
