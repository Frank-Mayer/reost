{
  "name": "reost",
  "repository": "https://github.com/Reost-MC/reost-website.git",
  "author": "Frank Mayer <frank.mayer1907@protonmail.com>",
  "license": "MIT",
  "private": false,
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "last 3 and_chr versions",
    "last 3 chrome versions",
    "last 3 opera versions",
    "last 3 ios_saf versions",
    "last 3 safari versions"
  ],
  "scripts": {
    "dev": "parcel serve ./src/index.html ./src/**/*.html",
    "clean": "rm -rf ./.parcel-cache && rm -rf ./dist",
    "build": "parcel build ./src/index.html ./src/**/*.html --no-source-maps --no-cache"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.3.2",
    "autoprefixer": "^10.4.4",
    "parcel": "^2.3.2",
    "postcss": "^8.4.12",
    "sass": "^1.50.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@frank-mayer/magic": "^2.0.1",
    "@frank-mayer/photon": "^1.2.1",
    "bootstrap-icons": "^1.8.1",
    "normalize.css": "^8.0.1"
  }
}
